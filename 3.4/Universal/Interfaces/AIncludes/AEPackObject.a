	IF &TYPE('__AEPACKOBJECT__') = 'UNDEFINED' THEN
__AEPACKOBJECT__ SET 1

	IF &TYPE('__APPLEEVENTS__') = 'UNDEFINED' THEN
	include 'AppleEvents.a'
	ENDIF


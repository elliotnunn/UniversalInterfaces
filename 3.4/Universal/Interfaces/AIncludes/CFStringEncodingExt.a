															; Various national standards begin at 0x600 
															;  kCFStringEncodingASCII = 0x0600, defined in CoreFoundation/CFString.h 
kCFStringEncodingJIS_X0201_76	EQU		$0620
kCFStringEncodingJIS_X0208_83	EQU		$0621
kCFStringEncodingJIS_X0208_90	EQU		$0622
kCFStringEncodingJIS_X0212_90	EQU		$0623

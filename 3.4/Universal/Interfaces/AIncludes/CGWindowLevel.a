	IF &TYPE('__CGWINDOWLEVEL__') = 'UNDEFINED' THEN
__CGWINDOWLEVEL__ SET 1

	IF &TYPE('__CGBASE__') = 'UNDEFINED' THEN
	include 'CGBase.a'
	ENDIF

; 
; * Windows may be assigned to a particular level. When assigned to a level,
; * the window is ordered relative to all other windows in that level.
; * Windows with a higher level are sorted in front of windows with a lower
; * level.
; *
; * A common set of window levels is defined here for use within higher
; * level frameworks.  The levels are accessed via a key and function,
; * so that levels may be changed or adjusted in future releases without
; * breaking binary compatability.
; 

; typedef int32_t 						CGWindowLevel

; typedef int32_t 						CGWindowLevelKey


; typedef long							_CGCommonWindowLevelKey
kCGBaseWindowLevelKey			EQU		0
kCGMinimumWindowLevelKey		EQU		1
kCGDesktopWindowLevelKey		EQU		2
kCGBackstopMenuLevelKey			EQU		3
kCGNormalWindowLevelKey			EQU		4
kCGFloatingWindowLevelKey		EQU		5
kCGTornOffMenuWindowLevelKey	EQU		6
kCGDockWindowLevelKey			EQU		7
kCGMainMenuWindowLevelKey		EQU		8
kCGStatusWindowLevelKey			EQU		9
kCGModalPanelWindowLevelKey		EQU		10
kCGPopUpMenuWindowLevelKey		EQU		11
kCGDraggingWindowLevelKey		EQU		12
kCGScreenSaverWindowLevelKey	EQU		13
kCGMaximumWindowLevelKey		EQU		14
kCGNumberOfWindowLevelKeys		EQU		15					; Internal bookkeeping; must be last 

; *  CGWindowLevelForKey()
; *  
; *  Availability:
; *    Non-Carbon CFM:   not available
; *    CarbonLib:        not available
; *    Mac OS X:         in version 10.0 or later
; 

;
; extern CGWindowLevel CGWindowLevelForKey(CGWindowLevelKey key)
;
	IF TARGET_OS_MAC ** TARGET_RT_MAC_CFM THEN
		IMPORT_CFM_FUNCTION CGWindowLevelForKey
	ENDIF

;  number of levels above kCGMaximumWindowLevel reserved for internal use 

kCGNumReservedWindowLevels		EQU		16
	ENDIF ; __CGWINDOWLEVEL__ 


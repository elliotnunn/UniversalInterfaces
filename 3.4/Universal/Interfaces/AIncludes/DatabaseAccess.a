;
; pascal OSErr DBIdle(void )
;
	IF TARGET_OS_MAC ** TARGET_CPU_68K ** ¬ TARGET_RT_MAC_CFM THEN
		Macro
		_DBIdle
			move.w              #$00FF,D0
			dc.w            	$A82F
		EndM
	ELSEIF TARGET_OS_MAC ** TARGET_RT_MAC_CFM THEN
		IMPORT_CFM_FUNCTION DBIdle
	ENDIF



	ENDIF	; CALL_NOT_IN_CARBON
	ENDIF ; __DATABASEACCESS__ 


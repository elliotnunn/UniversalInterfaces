	IF &TYPE('__IBCARBONRUNTIME__') = 'UNDEFINED' THEN
__IBCARBONRUNTIME__ SET 1

	IF &TYPE('__CFSTRING__') = 'UNDEFINED' THEN
	include 'CFString.a'
	ENDIF
	IF &TYPE('__CFBUNDLE__') = 'UNDEFINED' THEN
	include 'CFBundle.a'
	ENDIF
	IF &TYPE('__MACWINDOWS__') = 'UNDEFINED' THEN
	include 'MacWindows.a'
	ENDIF
	IF &TYPE('__CONTROLDEFINITIONS__') = 'UNDEFINED' THEN
	include 'ControlDefinitions.a'
	ENDIF

kIBCarbonRuntimeCantFindNibFile	EQU		-10960
kIBCarbonRuntimeObjectNotOfRequestedType EQU -10961
kIBCarbonRuntimeCantFindObject	EQU		-10962

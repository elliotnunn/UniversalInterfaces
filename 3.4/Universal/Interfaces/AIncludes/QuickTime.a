	IF &TYPE('__QUICKTIME__') = 'UNDEFINED' THEN
__QUICKTIME__ SET 1

	IF &TYPE('__CARBON__') = 'UNDEFINED' THEN
	include 'Carbon.a'
	ENDIF


	IF &TYPE('__MEDIAHANDLERS__') = 'UNDEFINED' THEN
	include 'MediaHandlers.a'
	ENDIF
	IF &TYPE('__MOVIES__') = 'UNDEFINED' THEN
	include 'Movies.a'
	ENDIF
	IF &TYPE('__MOVIESFORMAT__') = 'UNDEFINED' THEN
	include 'MoviesFormat.a'
	ENDIF
	IF &TYPE('__QUICKTIMEVR__') = 'UNDEFINED' THEN
	include 'QuickTimeVR.a'
	ENDIF
	IF &TYPE('__QUICKTIMEVRFORMAT__') = 'UNDEFINED' THEN
	include 'QuickTimeVRFormat.a'
	ENDIF
	IF &TYPE('__IMAGECOMPRESSION__') = 'UNDEFINED' THEN
	include 'ImageCompression.a'
	ENDIF
	IF &TYPE('__IMAGECODEC__') = 'UNDEFINED' THEN
	include 'ImageCodec.a'
	ENDIF
	IF &TYPE('__QUICKTIMEMUSIC__') = 'UNDEFINED' THEN
	include 'QuickTimeMusic.a'
	ENDIF
	IF &TYPE('__QUICKTIMECOMPONENTS__') = 'UNDEFINED' THEN
	include 'QuickTimeComponents.a'
	ENDIF
	IF &TYPE('__QUICKTIMESTREAMING__') = 'UNDEFINED' THEN
	include 'QuickTimeStreaming.a'
	ENDIF
	IF &TYPE('__QTSMOVIE__') = 'UNDEFINED' THEN
	include 'QTSMovie.a'
	ENDIF
	IF &TYPE('__QTSTREAMINGCOMPONENTS__') = 'UNDEFINED' THEN
	include 'QTStreamingComponents.a'
	ENDIF

	ENDIF ; __QUICKTIME__ 

